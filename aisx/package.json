{
  "name": "aisx",
  "version": "0.0.4",
  "description": "A JSX-based templating engine for generating structured prompts with TypeScript support",
  "license": "MIT",
  "author": "Dimitri Kennedy <dimitri@sick.email> (https://hack.dance)",
  "homepage": "https://github.com/hack-dance/aisx#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hack-dance/aisx.git"
  },
  "bugs": {
    "url": "https://github.com/hack-dance/aisx/issues"
  },
  "keywords": [
    "jsx",
    "template",
    "templating",
    "string",
    "renderer",
    "typescript",
    "bun",
    "node",
    "ai",
    "ml",
    "prompt-engineering",
    "llm",
    "prompts",
    "structured-prompts"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/src/types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "src/jsx-runtime/types.d.ts"
      ],
      "types": [
        "src/jsx-runtime/types.d.ts"
      ],
      "jsx-runtime": [
        "src/jsx-runtime/index.d.ts"
      ],
      "jsx-runtime/types": [
        "src/jsx-runtime/types.d.ts"
      ]
    }
  },
  "typings": "dist/src/types.d.ts",
  "files": [
    "dist",
    "README.md",
    "SETUP.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/src/jsx-runtime/index.js",
      "require": "./dist/src/jsx-runtime/index.js",
      "types": "./dist/src/jsx-runtime/index.d.ts"
    },
    "./jsx-runtime/types": {
      "import": "./dist/src/jsx-runtime/types.js",
      "require": "./dist/src/jsx-runtime/types.js",
      "types": "./dist/src/jsx-runtime/types.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/src/jsx-dev-runtime/index.js",
      "require": "./dist/src/jsx-dev-runtime/index.js",
      "types": "./dist/src/jsx-dev-runtime/index.d.ts"
    },
    "./tsconfig/aisx": {
      "import": "./dist/tsconfig/aisx.json",
      "require": "./dist/tsconfig/aisx.json"
    },
    "./global": {
      "types": "./dist/src/global.d.ts"
    },
    "./types": {
      "types": "./dist/src/global.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun ./build.ts",
    "publish:npm": "bun ./publish.ts",
    "publish:patch": "bun ./publish.ts patch",
    "publish:minor": "bun ./publish.ts minor",
    "publish:major": "bun ./publish.ts major",
    "lint:check": "TIMING=1 eslint src --max-warnings=0",
    "lint:fix": "eslint --fix src",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
